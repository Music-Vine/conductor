# Session secret for JWT signing (generate with: openssl rand -base64 32)
SESSION_SECRET=your-secret-key-here-minimum-32-characters

# API base URLs (optional, defaults to /api/mv and /api/ub)
NEXT_PUBLIC_MV_API_BASE=/api/mv
NEXT_PUBLIC_UB_API_BASE=/api/ub

# Backend API Integration (Phase 8)
# Set NEXT_PUBLIC_USE_REAL_API=true to switch from mock data to real backend
NEXT_PUBLIC_USE_REAL_API=false
BACKEND_API_BASE_URL=https://api.internal.example.com
BACKEND_API_SECRET=your-backend-api-secret-here
